{
  "name": "ng-apimock",
  "description": "Node plugin that provides the ability to use scenario based api mocking of angular apps",
  "version": "1.1.2",
  "homepage": "https://mdasberg.github.io/ng-apimock",
  "author": {
    "name": "Mischa Dasberg",
    "email": "mischa@dasberg.nl"
  },
  "contributors": [
    "Justus Romijn <justus.romijn@gmail.com>",
    "Richard Rijnberk <r.rijnberk@gmail.com>",
    "Frank Merema <frankmerema@gmail.com",
    "Jeroen van de Pol <jeroenvandepol@gmail.com"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdasberg/ng-apimock.git"
  },
  "bugs": {
    "url": "https://github.com/mdasberg/ng-apimock/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mdasberg/ng-apimock/blob/master/LICENSE-MIT"
    }
  ],
  "main": "tasks/index.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "local": "node node_modules/jasmine-node/bin/jasmine-node test/unit/*.spec.js && node node_modules/protractor/bin/protractor test/protractor/config/protractor-local.conf.js",
    "travis": "node node_modules/jasmine-node/bin/jasmine-node test/unit/*.spec.js && node node_modules/protractor/bin/protractor test/protractor/config/protractor-travis.conf.js"
  },
  "dependencies": {
    "angular": "1.5.7",
    "angular-mocks": "1.5.7",
    "angular-resource": "1.5.7",
    "async": "2.0.0-rc.6",
    "fs-extra": "^0.30.0",
    "glob": "7.0.5",
    "hooker": "^0.2.3",
    "lodash": "4.13.1",
    "path": "0.12.7",
    "sync-request": "3.0.1",
    "url-join": "^1.1.0"
  },
  "keywords": [
    "angular",
    "api",
    "mock",
    "scenario",
    "httpbackend"
  ],
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "connect": "^3.4.1",
    "cucumber": "1.2.0",
    "cucumberjs-junitxml": "0.0.12",
    "hooker": "^0.2.3",
    "jasmine-node": "^1.14.5",
    "protractor": "^3.3.0",
    "protractor-cucumber-framework": "^0.6.0",
    "serve-static": "^1.11.1"
  }
}
