{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "types": [
      "node", "glob", "async", "fs-extra"
    ]
  },
  "exclude": [
    "built",
    "node_modules",
    "results",
    "test"
  ]
}